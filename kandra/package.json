{
    "main": "electron/electron.cjs",
    "name": "application1",
    "private": true,
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev:react": "cross-env NODE_ENV=development vite --force",
        "dev:electron": "cross-env NODE_ENV=development wait-on http://127.0.0.1:5173 && electron . --no-sandbox",
        "dev": "concurrently -k \"npm run dev:react\" \"npm run dev:electron\"",
        "build": "vite build",
        "electron": "electron .",
        "start": "npm run dev",
        "clean": "rimraf dist userData node_modules/.vite node_modules/.cache",
        "rebuild": "npm run clean && npm install && npm run build"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.7",
        "@mui/material": "^7.3.7",
        "@mui/system": "^7.3.7",
        "@mui/x-charts": "^8.25.0",
        "boxicons": "^2.1.4",
        "framer-motion": "^12.29.2",
        "lucide-react": "^0.563.0",
        "react": "^19.2.0",
        "react-datepicker": "^9.1.0",
        "react-dom": "^19.2.0",
        "react-icons": "^5.5.0",
        "react-router-dom": "^7.12.0",
        "react-tooltip": "^5.30.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "concurrently": "^9.2.1",
        "cross-env": "^10.1.0",
        "electron": "^40.0.0",
        "electron-devtools-installer": "^4.0.0",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "rimraf": "^6.1.2",
        "vite": "^7.2.4",
        "wait-on": "^9.0.3"
    }
}
